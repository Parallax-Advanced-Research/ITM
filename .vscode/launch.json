    "version": "0.2.0",
    "configurations": [
        {
            // has tagging, treatment and pain_med
            "name": "Tad Tester MJ1",
            "type": "debugpy",
            "request": "launch",
            "program": "tad_tester.py",
            "console": "integratedTerminal",
            "args": "--session_type adept --scenario DryRunEval.MJ1 --selector keds --training --assessor triage --kdma \"Moral judgement=0.5\" --decision_verbose --no-mc --no-br --no-bayes --no-ebd",
            "env": {
                "PYTHONUNBUFFERED": "1"
            }
        },
        {
            // has tagging, treatment and pain_med
            "name": "Log Tad Tester MJ1",
            "type": "debugpy",
            "request": "launch",
            "program": "tad_tester_log.py",
            "console": "integratedTerminal",
            "args": "--session_type adept --scenario DryRunEval.MJ1 --selector keds --training --assessor triage --kdma \"Moral judgement=0.5\" --decision_verbose --no-mc --no-bayes --no-ebd"
        },
        {
            // has tagging, treatment and pain_med
            "name": "Tad Tester MJ1",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/tad_tester.py",
            "console": "integratedTerminal",
            "args": [
                "--session_type",
                "adept",
                "--scenario",
                "DryRunEval.MJ1",
                "--selector",
                "keds",
                "--training",
                "--assessor",
                "triage",
                "--kdma",
                "Moral judgement=0.5",
                "--decision_verbose",
                "--no-mc",
                "--no-br",
                "--no-bayes",
                "--no-ebd"
            ],
            "env": {
                "PYTHONUNBUFFERED": "1"
            },
            "redirectOutput": true,
        },
    ]
